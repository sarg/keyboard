{
    "keyboard_name": "UnitZero",
    "manufacturer": "Mommy Corp",
    "url": "https://github.com/sarg/keyboard",
    "maintainer": "sarg",
    "tags": ["split", "ortho"],
    "usb": {
        "vid": "0xFEED",
        "pid": "0x0000",
        "device_version": "0.0.1"
    },
    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        "via": true,
        "console": false,
        "command": false,
        "encoder_map": true,
        "nkro": false,
        "encoder": true
    },
    "processor": "atmega32u4",
    "bootloader": "qmk-dfu",
    "diode_direction": "COL2ROW",

    "qmk_lufa_bootloader": {
        "esc_output": "F6",
        "esc_input": "D7",
        "led": "B0"
    },

    "matrix_pins": {
        "cols": [ "F6", "F7", "B1", "B3", "B2", "B6" ],
        "rows": [ "D7", "E6", "B4", "B5" ]
    },

    "tapping": {
        "hold_on_other_key_press": true,
        "term": 200
    },

    "encoder": {
        "rotary": [
            {"pin_a": "D4", "pin_b": "C6"}
        ]
    },

    "split": {
        "soft_serial_pin": "D0",
        "enabled": true
    },

    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 1.5},
                {"matrix": [0, 1], "x": 1, "y": 1.25},
                {"matrix": [0, 2], "x": 2, "y": 0.5},
